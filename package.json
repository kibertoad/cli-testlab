{
  "name": "cli-testlab",
  "version": "1.7.0",
  "author": {
    "name": "Igor Savin",
    "web": "https://github.com/kibertoad"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "del-cli dist/ && tsc && copyfiles test/util/apps/*.js dist",
    "prepublishOnly": "npm run build",
    "test": "jest --config=jest.config.json --runInBand",
    "test:coverage": "jest --config=jest.config.json --coverage --runInBand",
    "test:ci": "npm run eslint && npm run test:coverage",
    "eslint": "eslint --format codeframe \"lib/**/*.ts\" \"test/**/*.ts\" \"test/**/*.js\"",
    "prettier": "prettier --write \"{lib,test}/**/*.{js,ts}\""
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/cli-testlab.git"
  },
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "del": "^4.1.1",
    "globule": "^1.2.1",
    "jake": "^8.1.1",
    "mkdirp-promise": "^5.0.1",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "@types/globule": "^1.1.3",
    "@types/jest": "^24.0.15",
    "@types/node": "^10.14.10",
    "@types/rimraf": "^2.0.2",
    "commander": "^2.20.0",
    "copyfiles": "^2.1.0",
    "del-cli": "^2.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2"
  }
}
