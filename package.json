{
  "name": "cli-testlab",
  "version": "1.8.0",
  "author": {
    "name": "Igor Savin",
    "web": "https://github.com/kibertoad"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && copyfiles test/util/apps/*.js dist",
    "prepublishOnly": "npm run build",
    "test": "jest --config=jest.config.json --runInBand",
    "test:coverage": "jest --config=jest.config.json --coverage --runInBand",
    "test:ci": "npm run eslint && npm run test:coverage",
    "eslint": "eslint --format codeframe \"lib/**/*.ts\" \"test/**/*.ts\" \"test/**/*.js\"",
    "prettier": "prettier --write \"{lib,test}/**/*.{js,ts}\""
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/cli-testlab.git"
  },
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "del": "^4.1.1",
    "globule": "^1.3.0",
    "jake": "^8.1.1",
    "mkdirp-promise": "^5.0.1",
    "rimraf": "^2.7.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "@types/globule": "^1.1.3",
    "@types/jest": "^24.0.25",
    "@types/node": "^10.17.13",
    "@types/rimraf": "^2.0.2",
    "commander": "^4.1.0",
    "copyfiles": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.4"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist/*"
  ]
}
