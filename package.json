{
  "name": "cli-testlab",
  "version": "3.0.0",
  "description": "Test framework for CLI utilities",
  "author": {
    "name": "Igor Savin",
    "web": "https://github.com/kibertoad"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "del-cli dist/ && tsc && copyfiles test/util/apps/*.js dist",
    "prepublishOnly": "npm run build",
    "test": "jest --config=jest.config.json --runInBand",
    "test:coverage": "jest --config=jest.config.json --coverage --runInBand",
    "test:ci": "npm run lint && npm run test:coverage",
    "lint": "eslint --format codeframe \"lib/**/*.ts\" \"test/**/*.ts\" \"test/**/*.js\" index.ts",
    "prettier": "prettier --write \"{lib,test}/**/*.{js,ts}\" index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/cli-testlab.git"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "del": "^5.1.0",
    "globule": "^1.3.2",
    "rimraf": "^5.0.1",
    "tslib": "^2.5.3"
  },
  "devDependencies": {
    "@types/globule": "^1.1.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^14.18.48",
    "@types/rimraf": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "commander": "^10.0.1",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.0.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.2.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "5.1.3"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist/lib/*",
    "dist/index.js",
    "dist/index.d.ts"
  ],
  "keywords": [
    "cli",
    "test",
    "tests",
    "testing",
    "framework",
    "commandline",
    "terminal"
  ]
}
