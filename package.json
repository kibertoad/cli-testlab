{
  "name": "cli-testlab",
  "version": "2.0.0",
  "author": {
    "name": "Igor Savin",
    "web": "https://github.com/kibertoad"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "del-cli dist/ && tsc && copyfiles test/util/apps/*.js dist",
    "prepublishOnly": "npm run build",
    "test": "jest --config=jest.config.json --runInBand",
    "test:coverage": "jest --config=jest.config.json --coverage --runInBand",
    "test:ci": "npm run lint && npm run test:coverage",
    "lint": "eslint --format codeframe \"lib/**/*.ts\" \"test/**/*.ts\" \"test/**/*.js\" index.ts",
    "prettier": "prettier --write \"{lib,test}/**/*.{js,ts}\" index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/cli-testlab.git"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "del": "^5.1.0",
    "globule": "^1.3.2",
    "jake": "^8.1.1",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "@types/globule": "^1.1.3",
    "@types/jest": "^26.0.5",
    "@types/node": "^14.0.23",
    "@types/rimraf": "^3.0.0",
    "commander": "^5.1.0",
    "copyfiles": "^2.3.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.3",
    "typescript": "3.9.7"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist/lib/*",
    "dist/index.js",
    "dist/index.d.ts"
  ]
}
