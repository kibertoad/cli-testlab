{
  "name": "cli-testlab",
  "version": "1.0.0",
  "author": {
    "name": "Igor Savin",
    "web": "https://github.com/kibertoad"
  },
  "scripts": {
    "test": "mocha ./test/unit",
    "test:cli": "jake -f test/integration/Jakefile",
    "test:coverage": "nyc npm test",
    "test:ci": "npm run lint && npm run test:coverage && npm run test-cli",
    "lint": "eslint .",
    "prettier": "prettier --write \"{lib,test}/**/*.{js,ts}\" \"index.js\""
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/cli-testlab.git"
  },
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "jake": "^8.1.1",
    "mkdirp-promise": "^5.0.1",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "commander": "^2.19.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "prettier": "^1.16.4",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0"
  },
  "nyc": {
    "description": "test coverage",
    "exclude": [
      "node_modules",
      "test"
    ],
    "reporter": [
      "text"
    ],
    "cache": true,
    "all": true,
    "check-coverage": false,
    "lines": 70,
    "statements": 70,
    "functions": 80,
    "branches": 55
  }
}
