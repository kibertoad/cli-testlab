{
  "name": "cli-testlab",
  "version": "1.0.0",
  "author": {
    "name": "Igor Savin",
    "web": "https://github.com/kibertoad"
  },
  "scripts": {
    "build": "del-cli dist/ && tsc",
    "test": "jest --config=jest.config.json --runInBand",
    "test:coverage": "jest --config=jest.config.json --coverage --runInBand",
    "test:ci": "npm run eslint && npm run test:coverage && npm run test:cli",
    "test:cli": "jake -f test/integration/Jakefile",
    "eslint": "eslint --format codeframe \"lib/**/*.ts\" \"test/**/*.ts\" \"test/**/*.js\"",
    "prettier": "prettier --write \"{lib,test}/**/*.{js,ts}\""
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/cli-testlab.git"
  },
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "jake": "^8.1.1",
    "mkdirp-promise": "^5.0.1",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "@types/jest": "^24.0.11",
    "@types/node": "^10.14.6",
    "@types/rimraf": "^2.0.2",
    "commander": "^2.20.0",
    "del-cli": "^1.1.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.7.1",
    "prettier": "^1.17.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5"
  }
}
